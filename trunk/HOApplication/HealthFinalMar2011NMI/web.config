				<add name="HOMembershipProvider" type="System.Web.Security.SqlMembershipProvider" connectionStringName="HOSolConnectionString" requiresQuestionAndAnswer="false" minRequiredPasswordLength="1" minRequiredNonalphanumericCharacters="0" requiresUniqueEmail="False"/>
